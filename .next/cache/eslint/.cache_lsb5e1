/fm/Documents/Code/pacifico/frontend/components/Header/Header.tsx",["343"],[],"import Image, { StaticImageData } from \"next/image\";\nimport NavMenu from \"../NavMenu/NavMenu\";\nimport logo from \"../../public/Logo.png\";\nimport Link from \"next/link\";\nimport Social from \"./Social\";\n\ninterface Section {\n  section: string;\n  name: string;\n}\n\ninterface Props {\n  sections: Section[];\n  showMenu?: boolean;\n}\n\nconst Header = ({ sections, showMenu = true }: Props) => {\n  return (\n    <div>\n      <nav\n        id=\"nav\"\n        className=\"flex w-full top-0 fixed border-b-4 border-yellow place-items-center p-1 h-12 mt-0 bg-transparent z-40\"\n      >\n        {showMenu ? (\n          <NavMenu options={sections} />\n        ) : (\n          <span className=\"basis-1/4\"></span>\n        )}\n        <div\n          className={`m-auto bg-transparent justify-end sm:justify-center rounded-xl basis-3/4 sm:basis-2/4 flex`}\n        >\n          <Link href=\"/\">\n            <a className={`flex w-fit place-items-center pacifico`}>\n              <h1\n                className={`text-xl pr-2 pacifico text-right sm:text-2xl sm:text-center text-yellow text-center font-bold header-font`}\n              >\n                Pacifico Basquet{\" \"}\n              </h1>\n              <div className=\"w-10 h-auto\">\n                <Image src={logo} height={50} width={50} layout=\"responsive\" />\n              </div>\n            </a>\n          </Link>\n        </div>\n        <Social responsive=\"social-media\" />\n      </nav>\n    </div>\n  );\n};\n\nexport default Header;\n","/home/fm/Documents/Code/pacifico/frontend/components/Header/Social.tsx",["344"],[],"import React, { useState } from \"react\";\nimport Image from \"next/image\";\nimport { socialMedia } from \"../../utils/constants\";\n\ninterface Props {\n  responsive?: string;\n}\nconst Social = ({ responsive }: Props) => {\n  return (\n    <div\n      className={`${responsive} flex overflow-hidden space-x-2 h-10 justify-end place-items-end text-yellow basis-1/4 m-auto`}\n    >\n      {socialMedia.map(({ url, image }) => (\n        <a\n          href={url}\n          key={url}\n          target=\"_blank\"\n          rel=\"noreferrer\"\n          className=\"rounded-full border border-1 border-white h-10 w-10 hover:bg-yellow\"\n        >\n          <Image\n            src={image}\n            width={50}\n            height={50}\n            className=\"object-cover p-2\"\n          />\n        </a>\n      ))}\n    </div>\n  );\n};\n\n// return (\n//   <div className=\"social-media basis-1/4 place-items-end m-auto justify-end flex\">\n//     <button className=\"w-12 mt-2\" onClick={() => setShow(true)}>\n//       <Image src={world} width={25} height={25} />\n//     </button>\n//   </div>\n// );\n// };\n\nexport default Social;\n","/home/fm/Documents/Code/pacifico/frontend/components/Header/index.ts",[],[],"/home/fm/Documents/Code/pacifico/frontend/components/Historia/Historia.tsx",[],[],"/home/fm/Documents/Code/pacifico/frontend/components/Historia/index.ts",[],[],"/home/fm/Documents/Code/pacifico/frontend/components/LoadingSpinner/LoadingSpinner.tsx",[],[],"/home/fm/Documents/Code/pacifico/frontend/components/LoadingSpinner/index.ts",[],[],"/home/fm/Documents/Code/pacifico/frontend/components/MyButton/MyButton.tsx",[],[],"/home/fm/Documents/Code/pacifico/frontend/components/NavMenu/MenuButton.tsx",[],[],"/home/fm/Documents/Code/pacifico/frontend/components/NavMenu/MenuItem.tsx",[],[],"/home/fm/Documents/Code/pacifico/frontend/components/NavMenu/MenuList.tsx",[],[],"/home/fm/Documents/Code/pacifico/frontend/components/NavMenu/NavMenu.tsx",["345"],[],"import React, { useState } from \"react\";\nimport Image from \"next/image\";\nimport menu from \"../../public/assets/menu.png\";\nimport MenuList from \"./MenuList\";\n\ninterface Option {\n  section: string;\n  name: string;\n}\n\ninterface Props {\n  options: Option[];\n}\n\nconst NavMenu = ({ options }: Props) => {\n  const [visible, setVisible] = useState(false);\n\n  const toggleVisible = () => {\n    setTimeout(() => setVisible(!visible), 200);\n  };\n\n  return (\n    <div\n      className={`${\n        visible ? \"\" : \"m-auto\"\n      } sm:basis-1/4 ml-6 block top-0 z-50`}\n    >\n      <button\n        id=\"dropdownDefault\"\n        data-dropdown-toggle=\"dropdown\"\n        className=\"max-h-10 text-white font-medium rounded-lg text-sm px-2 py-2.5 text-center inline-flex items-center\"\n        type=\"button\"\n        onClick={toggleVisible}\n      >\n        <Image src={menu} height={25} width={25} />\n      </button>\n      <MenuList\n        visible={visible}\n        options={options}\n        toggleVisible={toggleVisible}\n      />\n    </div>\n  );\n};\n\nexport default NavMenu;\n","/home/fm/Documents/Code/pacifico/frontend/components/NewCard/NewCard.tsx",[],[],"/home/fm/Documents/Code/pacifico/frontend/components/NewCard/index.ts",[],[],"/home/fm/Documents/Code/pacifico/frontend/components/News/News.tsx",[],[],"/home/fm/Documents/Code/pacifico/frontend/components/News/index.ts",[],[],"/home/fm/Documents/Code/pacifico/frontend/components/Newsletter/Newsletter.tsx",["346","347"],[],"import React, { useState } from \"react\";\nimport Image from \"next/image\";\nimport send from \"../../public/assets/send.png\";\nimport x from \"../../public/assets/x.png\";\nimport ok from \"../../public/assets/tick.png\";\nimport { BASE_API_URL } from \"../../utils/constants\";\n\nconst Newsletter = () => {\n  const [email, setEmail] = useState(\"\");\n  const [failed, setFailed] = useState(false);\n  const [sended, setSended] = useState(false);\n\n  const addToNewsletter = async (email: string) => {\n    try {\n      const data = await fetch(BASE_API_URL + \"newsletter/register\", {\n        method: \"POST\",\n        body: JSON.stringify({ email }),\n        headers: {\n          \"Content-Type\": \"application/json\",\n        },\n      });\n      const res = await data.json();\n      if (res.statusCode == 400) {\n        throw new Error(res.message);\n      }\n      return res;\n    } catch (e) {\n      throw e;\n    }\n  };\n\n  const handleSubmit = async (e: any) => {\n    e.preventDefault();\n    if (email.match(/^[\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4}$/g)) {\n      try {\n        await addToNewsletter(email);\n        setEmail(\"\");\n        setSended(true);\n        setTimeout(() => {\n          setSended(false);\n        }, 1500);\n      } catch (error) {\n        setFailed(true);\n        setTimeout(() => {\n          setFailed(false);\n        }, 1500);\n      }\n    } else {\n      setFailed(true);\n      setTimeout(() => {\n        setFailed(false);\n      }, 1500);\n    }\n  };\n\n  const handleChange = (e: any) => {\n    setEmail(e.target.value);\n  };\n\n  return (\n    <div className=\"bg-yellow relative shadow-lg shadow-yellow blob w-8/12 mx-auto my-8 flex justify-center flex-col\">\n      <h3 className=\"mb-0 font-bold md:font-medium mx-auto md:text-lg text-sm text-center md:text-left\">\n        {\" \"}\n        Suscribite a nuestro Newsletter\n      </h3>\n      <form\n        className={`mx-auto my-2 w-8/12 md:w-full${\n          failed && \"ring-red\"\n        } flex space-x-2 text-sm`}\n        onSubmit={handleSubmit}\n      >\n        {!failed && !sended && (\n          <>\n            <input\n              className={`p-2 w-full h-8 rounded-lg shadow email-inputs`}\n              placeholder=\"Tu email\"\n              value={email}\n              onChange={handleChange}\n              name=\"Email\"\n            ></input>\n            <button className=\"h-8 w-8\">\n              {\" \"}\n              <Image\n                src={send}\n                height={100}\n                width={100}\n                layout=\"responsive\"\n                alt=\"send\"\n              />{\" \"}\n            </button>\n          </>\n        )}\n\n        {failed && (\n          <div className=\"bg-red h-8 w-full rounded-lg px-2 flex justify-center\">\n            <Image src={x} height={30} width={30} />\n          </div>\n        )}\n        {sended && (\n          <div className=\"flex flex-col w-full justify-center\">\n            <div className=\"bg-green h-8 w-full rounded-lg px-2 flex justify-center\">\n              <Image src={ok} height={30} width={30} />\n            </div>\n            <div className=\"bg-white font-bold rounded w-fit p-2 mt-2 m-auto\">\n              {\" \"}\n              Revisa tu casilla de correo!{\" \"}\n            </div>\n          </div>\n        )}\n      </form>\n    </div>\n  );\n};\n\nexport default Newsletter;\n","/home/fm/Documents/Code/pacifico/frontend/components/Newsletter/index.ts",[],[],"/home/fm/Documents/Code/pacifico/frontend/components/Section/Section.tsx",[],[],"/home/fm/Documents/Code/pacifico/frontend/components/Section/index.ts",[],[],"/home/fm/Documents/Code/pacifico/frontend/components/Team/Team.tsx",["348"],[],"import React from \"react\";\nimport Image from \"next/image\";\n\nfunction importAll(r: any) {\n  let images: any[] = [];\n  r.keys().map((item: any, index: any) => {\n    images[index] = r(item);\n  });\n  return images;\n}\n\nconst images = importAll(\n  require.context(\"../../public/assets/jugadores\", false, /\\.(png|jpe?g|svg)$/)\n);\n\ninterface Props {\n  id: string;\n}\nconst Team = ({ id }: Props) => {\n  return (\n    <div\n      id={id}\n      className=\"flex flex-col justify-around mt-8 w-11/12 m-auto justify-start\"\n    >\n      <h3 className=\"text-black text-center header-font font-bold text-left text-3xl\">\n        {\" \"}\n        Nuestro equipo{\" \"}\n      </h3>\n      <h5 className=\"text-gray text-center\">Prefederal Masculino - 2022</h5>\n      <div className=\"flex flex-row w-full my-4 overflow-x-wrap overflow-y-hidden place-items-center justify-start space-x-2\">\n        {images.map((im) => {\n          return (\n            <div\n              key={im}\n              className=\"bg-tra